<launch>
  <arg name="mav_name" default="iris"/>
  <arg name="command_input" default="2" />
  <arg name="gazebo_simulation" default="false" /> <!-- regular real life testing is always false -->

  <arg name="file_waypoints" value="$(find controllers)/policy/waypoints_demo.txt"/>
  <arg name="file_waypoints_stamped" value="$(find controllers)/policy/waypoints_stamped.txt"/>

  <param name="use_sim_time" value="true" />	<!-- remove this fker when dealing w the real thing -->
  
  <node pkg="geometric_controller" type="geometric_controller_node" name="geometric_controller" output="screen">
  		    <param name="mav_name" type="string" value="$(arg mav_name)" />
          <!--<remap from="/geo_command/bodyrate_command" to="/mavros/setpoint_raw/attitude"/> --> <!-- remap thats why command/bodyrate is associated with attitude-->
          <param name="ctrl_mode" value="$(arg command_input)" />
          <param name="enable_sim" value="$(arg gazebo_simulation)" />
          <param name="enable_gazebo_state" value="true"/>
          <param name="max_acc" value="1.0" /> <!-- original 1.0 -->
          <param name="Kp_x" value="5.5" /> <!-- original 8.0 -->
          <param name="Kp_y" value="5.5" /> <!-- original 8.0 -->
          <param name="Kp_z" value="10.0" /> <!-- original 10.0 -->
	  <param name="Kv_x" value="2.0" /> <!-- used to be 1.5, original params -->
          <param name="Kv_y" value="2.0" /> <!-- original 1.5 -->
          <param name="Kv_z" value="3.3" /> <!-- original 3.3 -->
	  <param name="yaw_heading" value="0.0" />
          <param name="normalizedthrust_constant" value="0.05" />
          <param name="normalizedthrust_offset" value="0.1" />
          <param name="attctrl_constant" value="1.0" />	
  </node>

  <node pkg="trajectory_publisher" type="trajectory_publisher" name="trajectory_publisher" output="screen">
        <param name="trajectory_type" value="1" /> <!-- linear graph is 0, circle is 1 -->
        <param name="shape_omega" value="1.2" />
        <param name="initpos_z" value="2.0" />
        <param name="reference_type" value="2" />
  </node>

  <node pkg="geometric_controller" name="controller_test" type="controller_test" output="screen">
  </node> 

  <node name="trajectory" pkg="controllers" type="trajectory" output="screen" args="">  <!-- newly added from andriy's publisher -->
	<param name="file_waypoints" value="$(arg file_waypoints)" /> 
	<param name="file_waypoints_stamped" value="$(arg file_waypoints_stamped)" />
  </node>

  <node name="potential_fields" pkg="artificial_potential_fields" type="apf" output="screen" args=""> -->
	<param name="UAV_radius" value="1.0" /> -->
	<!-- <remap from="/uav/attractive_velocity" to="/$(arg vehicle)/attractive_velocity"/> -->
	<!-- <remap from="/potential_fields/velocity" to="/$(arg vehicle)/command_velocity"/> -->
  </node> -->  

  <!-- Launch rqt_reconfigure -->
  <node pkg="rqt_reconfigure" type="rqt_reconfigure" output="screen" name="rqt_reconfigure" /> -->

  <!--<node name="rviz" pkg="rviz" type="rviz" output="screen" args="d $(find artificial_potential_fields)/rviz/rviz.rviz"/>-->
  
  <!--  <node pkg="rqt_multiplot" type="rqt_multiplot" output="screen" name="rqt_multiplot" /> -->

</launch>
